#include <iostream>
//void is universal to not return a declared variable 
//if returning a variable the function most be declare that variable in the heading
//main() returns an integer. the heading declares: int main()

void showMessage()
{
    std::cout<<"Hello!"<<std::endl;
}

void displayNum(int num)
{
    std::cout<<"The value is "<< num << std::endl;
}


int main()
{
    showMessage();
    displayNum(5);
    return 0;
}
